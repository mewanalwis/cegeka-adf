{
	"name": "stg_rest_api_requests",
	"properties": {
		"activities": [
			{
				"name": "get_traffic_detectors",
				"description": "Get a list of traffic detectors ",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['geometry']['coordinates'][0]"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['geometry']['type']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['controllerid']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['detectorid']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['name']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['description']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['degree']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['latitude']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['longitude']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['x_sweref991500']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['properties']['y_sweref991500']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['features'][0]['type']"
								}
							},
							{
								"source": {
									"path": "$['detectorListGeoJSON']['type']"
								}
							},
							{
								"source": {
									"path": "$['status']"
								}
							},
							{
								"source": {
									"path": "$['message']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ds_traffic_lights_source",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_traffic_lights_staging",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "DP500"
		},
		"annotations": []
	}
}